# use custom 404 page
ErrorDocument 404 /404.html

# blog/(tags|categories)/[0-9]+
<FilesMatch "^[0-9]+$">
    DefaultType text/html
</FilesMatch>

# redirect with mod_rewrite
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  RewriteRule ^profile.html  http://about.me/sharkpp [L,R]
  RewriteRule ^log/(.*)$  - [R=404,L]

  RewriteRule ^blog/([0-9]+)/([0-9]+)/([0-9]+).html$ /blog/$1/$2/$3/ [R=302,L]
  RewriteRule ^blog/([0-9]+)/([0-9]+).html$          /blog/$1/$2/    [R=302,L]
  RewriteRule ^blog/([0-9]+).html$                   /blog/$1/       [R=302,L]
  RewriteRule ^(.+).htm$                             /$1.html        [R=302,L]
</IfModule>

